/* Catalan Number */
/* Time Complexity O(N^2) */
#include <iostream>
using namespace std;
#define mx 1000
long long C[mx] ;
long long catalan(int n)
{
    C[0] = 1 ;
    for(int i=1;i<=n;i++)
    {
        for(int j=0;j<i;j++)
        {
            C[i]+=C[j]*C[i-j-1];
        }
    }
    return C[n] ;
}
int main()
{
    int n ;
    cin >> n ;
    cout << catalan(n) << endl ;
    return 0;
}
