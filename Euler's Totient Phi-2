#include <stdio.h>
#define mx 100
int phi[mx+10];
void sieve_phi_function()
{
    for(int i=2;i<=mx;i++)
    {
        if(phi[i]==0)
        {
            phi[i]=i-1;
            for(int j=i+i;j<=mx;j=j+i)
            {
                if(phi[j]==0) phi[j]=j;
                phi[j]=phi[j]-phi[j]/i;
            }
        }
    }
}
int main()
{
    sieve_phi_function();
    for(int i=2;i<=mx;i++)
    {
        printf("%d = %d\n",i,phi[i]);
    }
    return 0;
}
